*.mp4
*.avi
*.mov
*.mkv
*.wmv
*.flv
*.webm

# Model weights
*.pt
*.pth
*.onnx
*.weights
*.h5
*.pb
models/*.pt
models/*.pth

# Data directories (keep structure, ignore contents)
data/input/*
data/output/*
!data/input/.gitkeep
!data/output/.gitkeep

